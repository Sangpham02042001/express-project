<%- include('../_layouts/adminheader') %> 

<h2 class="page-title">Edit Category</h2>
<a href="/admin/categories" class="btn btn-primary">Back to all categories</a>
<br><br>

<form method="POST" action="/admin/categories/edit-category/<%= id %> ">
    <div class="form-group">
        <label for="title">Title</label>
        <input type="text" class="from-control" placeholder="Title" name="title" id="title" value="<%= title %>" >
    </div>
    <button class="btn btn-default" type="submit">Submit</button>
</form>

<%- include('../_layouts/adminfooter') %> 